{
    "group/sysinfo": {
        "orientation": "horizontal",
        "modules": [
            "cpu",
            "separator",
            "memory"
        ]
    },
    // MODULES CONFIGURATION
    "cpu": {
        "interval": 10,
        "format": "CPU {}%  |",
        "max-length": 10
    },
    "memory": {
        "interval": 30,
        "format": "  RAM {}%",
        "max-length": 10
    }
}